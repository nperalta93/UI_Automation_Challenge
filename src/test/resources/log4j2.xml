<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <File name="LogFile" fileName="UI_tests.log">
            <PatternLayout>
                <Pattern>%d{HH:mm:ss.SSS} [%t][%M] %-5level %logger{36} - %msg%n</Pattern>
            </PatternLayout>
        </File>
    </Appenders>
    <Loggers>
        <Root level="trace">
            <AppenderRef ref="LogFile"/>
        </Root>
    </Loggers>
</Configuration>